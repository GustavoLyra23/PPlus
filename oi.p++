
interface Animal {
    funcao fazerSom(): Nulo;
}

classe SerVivo {
    var vivo: Logico = verdadeiro;
    
    funcao estaVivo(): Logico {
        return vivo;
    }
}

classe Cachorro estende SerVivo implementa Animal {
    var nome: Texto;
    
    funcao inicializar(n: Texto) {
        nome = n;
    }
    
    funcao fazerSom(): Nulo {
        escrever("Au au!");
    }
}

var rex = nova Cachorro("Rex");
escrever(rex.nome);
escrever(rex.estaVivo());
rex.fazerSom();